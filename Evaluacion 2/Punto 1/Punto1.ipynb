      "source": [
        "import numpy as np\n",
        "\n",
        "matriz = np.array([[1, 0, -1],\n",
        "                   [2, 0, 2],\n",
        "                   [8, 2, -3]])\n",
        "\n",
        "matriz_inversa = np.linalg.inv(matriz)\n",
